              <PricingRuleRef version="any" ref="tap:PricingRule:distance"/>
              <Itinerary>(Jena/Hof&gt; * Berlin Friedrichstr * (Buechen / Luebeck))</Itinerary>
              <SeriesType>stationToStation</SeriesType>
