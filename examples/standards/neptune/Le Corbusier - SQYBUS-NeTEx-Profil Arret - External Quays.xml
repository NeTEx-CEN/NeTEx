								<ClassInFrameRef nameOfClass="TypeOfFrame"/>
								<ClassInFrameRef nameOfClass="TypeOfValue"/>
							</classes>
						</TypeOfFrame>
						<TypeOfFrame version="1.01:FR-NETEX_ARRET-1.0" id="FR:TypeOfFrame:NETEX_ARRET">
