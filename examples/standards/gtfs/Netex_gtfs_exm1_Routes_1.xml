          <OperatorRef ref="mygtfsxm:Operator:DTA"/>
          <Presentation>
            <Colour>02FFFF</Colour>
            <TextColour>02FFFF</TextColour>
          </Presentation>
        </Line>
        <Line version="any" id="mygtfsxm:Line:CITY">
