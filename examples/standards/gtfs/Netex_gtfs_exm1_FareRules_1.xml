					<TypeOfFrameRef versionRef="gtfs:1.5" ref="gtfs:GTFS_Profile@Fare"/>
					<!--- ====Fare USAGE PARAMETERS======= -->
					<tariffs>
						<Tariff version="any" id="mygtfsxm:p">
							<TypeOfTariffRef versionRef="gtfs:1.5" ref="gtfs:flat"/>
