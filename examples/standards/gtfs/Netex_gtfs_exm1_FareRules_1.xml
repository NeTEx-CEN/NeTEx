				</Line>
				<Line version="any" id="mygtfsxm:STBA">
					<Name>Route STBA</Name>
				</Line>
				<Line version="any" id="mygtfsxm:BFC">
					<Name>Route BFC</Name>
				</Line>
				<Line version="any" id="mygtfsxm:AAMV">
					<Name>Route AAMV</Name>
				</Line>
			</lines>
			<groupsOfLines>
