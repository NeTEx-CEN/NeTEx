<?xml version="1.0" encoding="UTF-8"?>
<!-- 
==== Minimumseksempel for et stoppested med ett stopp ====== 
Eksempelet tar utgangspunkt i offisielle NeTEx-XML eksempler og viser et enkleste type stoppested for buss, uten fasiliteter, i en enveiskjørt gate-sløyfe.
    Denne består av:
    - STOP PLACE	NSR:StopPlace:10000000			Blystalia
	- QUAY			NSR:Quay:11111111		Plattform 2
	Frames / gruppering:
	- SITE FRAME inneholder STOP PLACE og QUAY
-->
<PublicationDelivery xmlns="http://www.netex.org.uk/netex" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://www.netex.org.uk/netex ../../../../xsd/NeTEx_publication.xsd">
	<PublicationTimestamp>2017-02-13T15:00:00.0Z</PublicationTimestamp>
	<ParticipantRef>NSR</ParticipantRef>
	<dataObjects>
		<SiteFrame version="any" id="NSR:SiteFrame:1">
			<stopPlaces>
				<!-- ===Stop=== -->
				<!-- Alle objekter skal ha nasjonal ID fra stoppestedsregisteret -->
				<StopPlace version="any" id="NSR:StopPlace:10000000">
					<Name>TODO</Name>
					<!-- Generelt tilgjengelig med rullestol -->
					<AccessibilityAssessment version="any" id="NSR:AccessibilityAssessment:1">
						<MobilityImpairedAccess>true</MobilityImpairedAccess>
						<limitations>
							<!-- Minimumskrav til UU-informasjon -->
							<AccessibilityLimitation>
								<WheelchairAccess>true</WheelchairAccess>
								<StepFreeAccess>true</StepFreeAccess>
							</AccessibilityLimitation>
						</limitations>
					</AccessibilityAssessment>
					<Covered>outdoors</Covered>
					<placeEquipments>
						<PassengerInformationEquipment id="NSR:PassengerInformationEquipment" version="any">
							<PassengerInformationFacilityList>nextStopIndicator</PassengerInformationFacilityList>
						</PassengerInformationEquipment>
						<TicketingEquipment id="NSR:TicketingEquipment:1" version="any">
							<VehicleModes>rail bus</VehicleModes>
							<NumberOfMachines>0</NumberOfMachines>
						</TicketingEquipment>
						<SanitaryEquipment id="NSR:SanitaryEquipment:1" version="any">
							<!-- TODO: Remove 'Gender' element, only required for validation against previous version of XSD -->
							<Gender>both</Gender>
							<SanitaryFacilityList>none</SanitaryFacilityList>
						</SanitaryEquipment>
						<GeneralSign id="NSR:GeneralSign:1" version="any">
							<PrivateCode>512</PrivateCode>
							<SignContentType>transportMode</SignContentType>
						</GeneralSign>
					</placeEquipments>
					<!-- 
TODO - modelling of:

luggagelockerfacility - bikeRack
place equipment - cycle parking equipment
-->
					<localServices>
						<LeftLuggageService id="leftluggageservice" version="any">
							<LockerFee>true</LockerFee>
						</LeftLuggageService>
					</localServices>
					<TransportMode>bus</TransportMode>
					<StopPlaceType>onstreetBus</StopPlaceType>
					<quays>
						<Quay version="any" id="NSR:Quay:11111111">
							<Centroid>
								<!-- Alltid WGS84 -->
								<Location srsName="WGS84">
									<Longitude>11.034508</Longitude>
									<Latitude>59.931081</Latitude>
								</Location>
							</Centroid>
							<Lighting>wellLit</Lighting>
							<PublicCode>2</PublicCode>
						</Quay>
					</quays>
				</StopPlace>
			</stopPlaces>
			<siteFacilitySets>
				<SiteFacilitySet id="sitefacilityset" version="any">
					<PassengerInformationEquipmentList>interactiveKiosk </PassengerInformationEquipmentList>
					<PassengerInformationFacilityList>passengerInformationDisplay nextStopIndicator</PassengerInformationFacilityList>
				</SiteFacilitySet>
			</siteFacilitySets>
		</SiteFrame>
	</dataObjects>
</PublicationDelivery>
