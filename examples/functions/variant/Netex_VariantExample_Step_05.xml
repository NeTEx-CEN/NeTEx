							<Name>From A to C, direct - Icy conditions, omit B </Name>
							<pointsInSequence>
								<StopPointInJourneyPattern version="001" id="mybus:StopPointInJourneyPattern:SP003_01" order="1">
									<ScheduledStopPointRef ref="mybus:ScheduledStopPoint:SSP0001A"/>
