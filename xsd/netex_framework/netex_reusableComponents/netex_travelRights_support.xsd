		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ReservationListOfEnumerations">
		<xsd:annotation>
			<xsd:documentation>List of Reservation values.</xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="ReservationEnumeration"/>
	</xsd:simpleType>
