	<!-- ======PATH DESCRIPTION ================================================== -->
	<xsd:group name="PathDescriptionGroup">
		<xsd:annotation>
			<xsd:documentation>Path related Elements of a  SITE LINK, OFF SITE LINK and  DECK  PATH LINK.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Description" type="MultilingualString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of PATH LINK.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="PathLinkElementGroup"/>
			<xsd:group ref="EnvironmentPropertiesGroup"/>
			<xsd:group ref="PathPropertiesGroup"/>
			<xsd:group ref="LinkDurationGroup"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======ACCESS SUMMARY ================================================== -->
	<xsd:complexType name="accessSummaries_RelStructure">
		<xsd:annotation>
			<xsd:documentation>A collection of one or more ACCESS SUMMARies.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="strictContainmentAggregationStructure">
				<xsd:sequence>
					<xsd:element ref="AccessSummary" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="AccessSummary">
		<xsd:annotation>
			<xsd:documentation>Summary of a feature used in NAVIGATION PATH.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="AccessSummary_VersionedChildStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="VersionedChildGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="AccessSummaryGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="AccessSummaryIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="AccessSummary_VersionedChildStructure">
		<xsd:annotation>
			<xsd:documentation>Type for ACCESS SUMMARY.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="VersionedChildStructure">
				<xsd:sequence>
					<xsd:group ref="AccessSummaryGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="AccessSummaryGroup">
		<xsd:annotation>
			<xsd:documentation>Elements of an ACCESS SUMMARY</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AccessFeatureType" type="AccessFeatureEnumeration">
				<xsd:annotation>
					<xsd:documentation>Type of access feature, e.g. lift, stairs,</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Count" type="xsd:nonNegativeInteger">
				<xsd:annotation>
					<xsd:documentation>Count of feature, e.g. number of lifts, stairs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Transition" type="TransitionEnumeration" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nature of access feature transition e.g. up or down.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- =====NAVIGATION PATH SUMMARY================================================== -->
	<xsd:group name="NavigationSummaryGroup">
		<xsd:annotation>
			<xsd:documentation>Elements of a NAVIGATION  that summarise properties of the set of PATH LINKs. Will state the strictest constraint found . e.g. if any link forbids wheelchair, the NAVIGATION PATH forbids wheelchair.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AccessibilityAssessment" minOccurs="0"/>
			<xsd:element name="AccessModes" type="AccessModeListOfEnumerations" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>MODEs of access which may used at associated place, e.g. foot access, bicycle access.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="summaries" type="accessSummaries_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Summaries of access features encountered in path.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransferDuration" type="TransferDurationStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total time needed to navigate path (May be derived from links).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
</xsd:schema>
