	<xsd:include schemaLocation="netex_serviceCalendar_support.xsd"/>
	<xsd:include schemaLocation="netex_dayType_version.xsd"/>
	<xsd:include schemaLocation="../netex_genericFramework/netex_assignment_version.xsd"/>
