				<Title>NeTEx EMPLOYEE identifier  types.</Title>
				<Type>Standard</Type>
			</Metadata>
		</xsd:appinfo>
		<xsd:documentation>NeTEx: EMPLOYEE identifier types.</xsd:documentation>
	</xsd:annotation>
	<!-- ==== EMPLOYEE ======================================================= -->
	<xsd:simpleType name="EmployeeIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a EMPLOYEE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="EmployeeRef" type="EmployeeRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a EMPLOYEE. +v2.0</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="EmployeeRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a EMPLOYEE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="EmployeeIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a Employee.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ==== QUALIFICATION ========================================== -->
	<xsd:simpleType name="QualificationIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a QUALIFICATION.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="QualificationRef" type="QualificationRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a QUALIFICATION. +v2.0</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="QualificationRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a QUALIFICATION.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="QualificationIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a Qualification.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<!-- =====TYPE OF QUALIFICATION ===================================================== -->
	<xsd:complexType name="typesOfQualificationRefs_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of TYPE OF QUALIFICATIONs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="oneToManyRelationshipStructure">
				<xsd:sequence>
					<xsd:element ref="TypeOfQualificationRef" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="TypeOfQualificationIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a TYPE OF QUALIFICATION.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TypeOfValueIdType"/>
	</xsd:simpleType>
	<xsd:element name="TypeOfQualificationRef" type="TypeOfQualificationRefStructure" substitutionGroup="TypeOfEntityRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a TYPE OF QUALIFICATION. +v2.0</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="TypeOfQualificationRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a reference to a TYPE OF QUALIFICATION.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="TypeOfValueRefStructure">
				<xsd:attribute name="ref" type="TypeOfQualificationIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Reference to identifier of a TYPE OF QUALIFICATION.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
</xsd:schema>
