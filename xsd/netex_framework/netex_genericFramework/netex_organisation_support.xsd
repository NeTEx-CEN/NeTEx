	<xsd:include schemaLocation="../netex_responsibility/netex_relationship_support.xsd"/>
	<xsd:include schemaLocation="../netex_utility/netex_utility_contact.xsd"/>
	<xsd:include schemaLocation="netex_zone_support.xsd"/>
