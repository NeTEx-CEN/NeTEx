	<xsd:element name="PricingRuleRef" type="PricingRuleRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a PRICING RULE.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="NameOfRefClassForPricingRuleRefStructure">
		<xsd:restriction base="NameOfRefClassForTypeOfValueRefStructure">
			<xsd:enumeration value="DiscountingRule"/>
			<xsd:enumeration value="LimitingRule"/>
			<xsd:enumeration value="PricingRule"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="PricingRuleRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a PRICING RULE.</xsd:documentation>
