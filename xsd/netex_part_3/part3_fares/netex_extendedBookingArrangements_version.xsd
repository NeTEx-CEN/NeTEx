					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<!--<xsd:sequence>
							<xsd:group ref="BookingArrangementGroup">
								<xsd:annotation>
									<xsd:documentation>Details for a BookingArrangement</xsd:documentation>
								</xsd:annotation>
							</xsd:group>
						</xsd:sequence>-->
						<xsd:sequence>
							<xsd:group ref="ServiceEligibilityConditionGroup">
								<xsd:annotation>
									<xsd:documentation>Details for ServiceEligbilityCondition</xsd:documentation>
								</xsd:annotation>
							</xsd:group>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="BookingArrangementIdType" use="required"/>
				</xsd:restriction>
			</xsd:complexContent>
