	<!-- ===== NON FARE PRODUCT==================================================== -->
	<xsd:complexType name="thirdPartyProductRefs_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for list of  references to a DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="oneToManyRelationshipStructure">
