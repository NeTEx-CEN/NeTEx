						<xsd:documentation>Identifier of a USAGE DISCOUNT RIGHT.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
