	<!-- ===ENTIITY IN VERSION IN FRAME====Used in FARE FRAME)=================================================== -->
	<xsd:group name="FareProductsInFrameGroup">
		<xsd:annotation>
			<xsd:documentation>Fare Product Elements contained in a FARE FRAME.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="usageParameters" type="usageParametersInFrame_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>FARE USAGE PARAMETERS in FRAME.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="accessRightParameterAssignments" type="accessRightParameterAssignmentsInFrame_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ACCESS RIGHT PARAMETER ASSIGNMENTs in frame.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fareProducts" type="fareProductsInFrame_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>FARE PRODUCTs in FRAME.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="fareProductsInFrame_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for containment in frame of FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="frameContainmentStructure">
				<xsd:sequence>
					<xsd:element ref="FareProduct_" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ====FARE PRODUCT=================================================== -->
	<xsd:element name="ServiceAccessRight_" type="DataManagedObjectStructure" abstract="false" substitutionGroup="PriceableObject_">
		<xsd:annotation>
			<xsd:documentation>An immaterial marketable element</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ServiceAccessRight" abstract="false" substitutionGroup="ServiceAccessRight_">
		<xsd:annotation>
			<xsd:documentation>An immaterial marketable element dedicated to accessing some services.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="ServiceAccessRight_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="ServiceAccessRightIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ServiceAccessRight_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for  SERVICE ACCESS RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PriceableObject_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="ServiceAccessRightGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="ServiceAccessRightGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a  SERVICE ACCESS RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="ProductDescriptionGroup"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="ProductDescriptionGroup">
		<xsd:annotation>
			<xsd:documentation>DescriptionElements for a  PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="PrivateCode" minOccurs="0"/>
			<xsd:element name="InfoUrl" type="xsd:anyURI" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>URL for further information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="documentLinks" type="infoLinks_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Timetable documents associated with the Product e.g pdf files</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ====FARE PRODUCT=================================================== -->
	<xsd:element name="FareProduct_" type="DataManagedObjectStructure" abstract="true" substitutionGroup="ServiceAccessRight_">
		<xsd:annotation>
			<xsd:documentation>An immaterial marketable element (access rights, discount rights etc), specific to a CHARGING MOMENT.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FareProduct" abstract="true" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>An immaterial marketable element (access rights, discount rights etc), specific to a CHARGING MOMENT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="FareProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="FareProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="FareProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ServiceAccessRight_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="FareProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="FareProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="FareProductDescriptionGroup"/>
			<xsd:group ref="FareProductRelationGroup">
				<xsd:annotation>
					<xsd:documentation>Related Elements for a FARE PRODUCT.</xsd:documentation>
				</xsd:annotation>
			</xsd:group>
			<xsd:group ref="FareProductValidityGroup"/>
			<xsd:group ref="FareProductPricingGroup"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="FareProductDescriptionGroup">
		<xsd:annotation>
			<xsd:documentation>DescriptionElements for a FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="ChargingMomentRef" minOccurs="0"/>
			<xsd:element ref="TypeOfFareProductRef" minOccurs="0"/>
			<xsd:element ref="ConditionSummary" minOccurs="0"/>
			<xsd:element ref="OperatorRef" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="FareProductValidityGroup">
		<xsd:annotation>
			<xsd:documentation>ValidityElements for a FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="OptimisedOneManyAssignmentGroup" minOccurs="0"/>
			<xsd:element name="validableElements" type="validableElements_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>VALIDABLE ELEMENTs making up FARE SERVICE ACCESS RIGHT.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="accessRightsInProduct" type="accessRightsInProduct_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Access rights given by product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="FareProductPricingGroup">
		<xsd:annotation>
			<xsd:documentation>Pricing Elements for a FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="prices" type="fareProductPrices_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>PRICEs  making up FARE PRODUCT.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="FareProductRelationGroup">
		<xsd:annotation>
			<xsd:documentation>Related Elements for a FARE PRODUCT PRICe.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="FareProductRef" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ====TYPE OF TFARE PRODUCT======================================== -->
	<xsd:complexType name="typeOfFareProducts_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of TYPE OF FARE PRODUCTs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="containmentAggregationStructure">
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="TypeOfFareProductRef"/>
					<xsd:element ref="TypeOfFareProduct">
						<xsd:annotation>
							<xsd:documentation>A TYPE OF FARE PRODUCT specifying access rights for the FARE PRODUCT ELEMENTs within the range of this UNIT: 0-5 km, 4-6 zones etc.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="TypeOfFareProduct" abstract="false" substitutionGroup="TypeOfEntity">
		<xsd:annotation>
			<xsd:documentation>A classification of FARE PRODUCTs expressing their general functionalities and local functional characteristics specific to the operator. Types of FARE PRODUCTs like e.g. throw-away ticket, throw-away ticket unit, value card, electronic purse allowing access, public transport credit card etc. may be used to define these categories.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="TypeOfFareProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="TypeOfValueGroup"/>
						</xsd:sequence>
						<xsd:sequence/>
					</xsd:sequence>
					<xsd:attribute name="id" type="TypeOfFareProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="TypeOfFareProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for TYPE OF FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="TypeOfEntity_VersionStructure">
				<xsd:group ref="TypeOfFareProductGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="TypeOfFareProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence/>
	</xsd:group>
	<!-- ==== SALES DISCOUNT RIGHT.=============================================== -->
	<xsd:element name="SaleDiscountRight" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT allowing a customer to benefit from discounts when purchasing SALES OFFER PACKAGEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="SaleDiscountRight_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="SaleDiscountRightGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="SaleDiscountRightIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="SaleDiscountRight_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for SALES DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FareProduct_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="SaleDiscountRightGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="SaleDiscountRightGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a SALES DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="OtherProductGroup" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="OtherProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a SALES DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="GeneralGroupOfEntitiesRef"/>
			<xsd:element ref="GeneralGroupOfEntities"/>
		</xsd:choice>
	</xsd:group>
	<!-- ==== ENTITLEMENT PRODUCT.=============================================== -->
	<xsd:element name="EntitlementProduct" abstract="false" substitutionGroup="ServiceAccessRight_">
		<xsd:annotation>
			<xsd:documentation>A precondition to access a service or to purchase a FARE PRODUCT issued by an organisation that may not be a PT operator (e.g. military card).</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="EntitlementProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntitlementProductGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="EntitlementProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="EntitlementProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for ENTITLEMENT PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ServiceAccessRight_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="EntitlementProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="EntitlementProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for an ENTITLEMENT PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="GeneralOrganisationRef" minOccurs="0"/>
			<xsd:element name="prices" type="fareProductPrices_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prices for ENTITLEMENT PRODUCT.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== THIRD PARTY PRODUCT.=============================================== -->
	<xsd:element name="ThirdPartyProduct" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT that is marketed together with a Public Transport Fare Product.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="ThirdPartyProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ThirdPartyProductGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="ThirdPartyProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ThirdPartyProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for THIRD PARTY PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FareProduct_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="ThirdPartyProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="ThirdPartyProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for an THIRD PARTY PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="OtherProductGroup" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== USAGE DISCOUNT RIGHT.=============================================== -->
	<xsd:element name="UsageDiscountRight" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT allowing a customer to benefit from discounts when consuming VALIDABLE ELEMENTs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="UsageDiscountRight_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="UsageDiscountRightGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="UsageDiscountRightIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:group name="UsageDiscountRightGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a USAGE DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence/>
	</xsd:group>
	<!-- ==== CAPPED DISCOUNT RIGHT.=============================================== -->
	<xsd:element name="CappedDiscountRight" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT allowing a customer to benefit from discounts when consuming VALIDABLE ELEMENTs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="CappedDiscountRight_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="SaleDiscountRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="CappedDiscountRightGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="CappedDiscountRightIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="CappedDiscountRight_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for CAPPED DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="SaleDiscountRight_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="CappedDiscountRightGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="CappedDiscountRightGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a CAPPED DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="cappingRules" type="cappingRules_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Maximum fare to charge.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== CAPPING RULE.=============================================== -->
	<xsd:complexType name="cappingRules_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of CAPPING RULEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="strictContainmentAggregationStructure">
				<xsd:sequence>
					<xsd:element ref="CappingRule" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="CappingRule" abstract="false" substitutionGroup="PriceableObject_">
		<xsd:annotation>
			<xsd:documentation>Rule about capping for a mode.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="CappingRule_VersionedChildStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:sequence>
								<xsd:element ref="keyList" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>A list of alternative Key values for an element.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element ref="Extensions" minOccurs="0"/>
								<xsd:element ref="BrandingRef" minOccurs="0"/>
							</xsd:sequence>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="CappingRuleGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="CappingRuleIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="CappingRule_VersionedChildStructure">
		<xsd:annotation>
			<xsd:documentation>Type for CAPPING RULE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PriceableObject_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="CappingRuleGroup">
						<xsd:annotation>
							<xsd:documentation>Elements for a CAPPING RULE.</xsd:documentation>
						</xsd:annotation>
					</xsd:group>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="CappingRuleGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a CAPPING RULE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="MaximumDistance" type="DistanceType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Maximum Distance for distance based Capping.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CappingPeriod" type="CappingPeriodEnumeration" default="day" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Period with which capping accumulation is done. Default is 'day'. A USAGE VALIDITY PERIDO parameter can be used to add a more specific definition.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CappedDiscountRightRef" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="PreassignedFareProductRef" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="ValidableElementRef" minOccurs="0"/>
			<xsd:group ref="OptimisedOneManyAssignmentGroup" minOccurs="0"/>
			<xsd:element name="prices" type="cappingRulePrices_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Maximum fare to charge.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== AMOUNT OF PRICE UNIT PRODUCT.=============================================== -->
	<xsd:complexType name="UsageDiscountRight_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for USAGE DISCOUNT RIGHT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FareProduct_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="UsageDiscountRightGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="AmountOfPriceUnitProduct" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT consisting in a stored value of PRICE UNITs: an amount of money on an electronic purse, amount of units on a value card etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="AmountOfPriceUnitProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="AmountOfPriceUnitProductGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="AmountOfPriceUnitProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="AmountOfPriceUnitProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for AMOUNT OF PRICE UNIT PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FareProduct_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="AmountOfPriceUnitProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="AmountOfPriceUnitProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a AMOUNT OF PRICE UNIT PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="PriceUnitRef" minOccurs="0"/>
			<xsd:element name="Amount" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of units</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== PREASSIGNED FARE PRODUC=============================================== -->
	<xsd:element name="PreassignedFareProduct" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT consisting of one or several VALIDABLE ELEMENTs, specific to a CHARGING MOMENT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="PreassignedFareProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PreassignedFareProductGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="PreassignedFareProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="PreassignedFareProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for PREASSIGNED FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FareProduct_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="PreassignedFareProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="PreassignedFareProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a PREASSIGNED FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence/>
	</xsd:group>
	<!-- ==== PREASSIGNED FARE PRODUC=============================================== -->
	<xsd:element name="SupplementProduct" abstract="false" substitutionGroup="FareProduct_">
		<xsd:annotation>
			<xsd:documentation>A FARE PRODUCT consisting of one or several VALIDABLE ELEMENTs, specific to a CHARGING MOMENT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="SupplementProduct_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PriceableObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="ServiceAccessRightGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="PreassignedFareProductGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="SupplementProductGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="PreassignedFareProductIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="SupplementProduct_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for PREASSIGNED FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PreassignedFareProduct_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="SupplementProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="SupplementProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a PREASSIGNED FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SupplementToFareProductRef" type="FareProductIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>FARE PRODUCT to which this is a sublement.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ====ACCESS RIGHT IN PRODUCT..============================= -->
	<xsd:complexType name="accessRightsInProduct_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of ACCESS RIGHT IN PRODUCTs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="strictContainmentAggregationStructure">
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="AccessRightInProductRef"/>
					<xsd:element ref="AccessRightInProduct"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="AccessRightInProduct" type="AccessRightInProduct_VersionedChildStructure" abstract="false" substitutionGroup="VersionedChild">
		<xsd:annotation>
			<xsd:documentation>A VALIDABLE ELEMENT as a part of a PRE-ASSIGNED FARE PRODUCT, including its possible order in the set of all VALIDABLE ELEMENTs grouped together to define the access right assigned to that PRE-ASSIGNED FARE PRODUCT.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AccessRightInProduct_VersionedChildStructure">
		<xsd:annotation>
			<xsd:documentation>Type for ACCESS RIGHT IN PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FareElementInSequence_VersionedChildStructure">
				<xsd:sequence>
					<xsd:group ref="AccessRightInProductGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="AccessRightInProductGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a ACCESS RIGHT IN PRODUCT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="ValidableElementRef" minOccurs="0"/>
			<xsd:element ref="PreassignedFareProductRef" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:element name="ChargingMoment" abstract="false" substitutionGroup="TypeOfValue">
		<xsd:annotation>
			<xsd:documentation>A classification of FARE PRODUCTs according to the payment method and the account location: pre-payment with cancellation (throw-away), pre-payment with debit on a value card, pre-payment without consumption registration (pass), post-payment etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="ChargingMoment_ValueStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="TypeOfValueGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="ChargingMomentIdType">
						<xsd:annotation>
							<xsd:documentation>Identifier of CHARGING MOMENT.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ChargingMoment_ValueStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for a CHARGING MOMENT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="TypeOfValue_VersionStructure"/>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- == FARE PRICEs.============================= -->
	<xsd:complexType name="fareProductPrices_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of FARE PRODUCT PRICEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="strictContainmentAggregationStructure">
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="FareProductPriceRef"/>
					<xsd:element ref="CellRef_"/>
					<xsd:element ref="FareProductPrice"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="FareProductPrice" abstract="false" substitutionGroup="FarePrice_">
		<xsd:annotation>
			<xsd:documentation>A set of all possible price features of a FARE PRODUCT default total price, discount in value or percentage etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="FareProductPrice_VersionedChildStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="VersionedChildGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FarePriceGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FareProductPriceGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="FareProductPriceIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="FareProductPrice_VersionedChildStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a FARE PRODUCT PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FarePrice_VersionedChildStructure">
				<xsd:sequence>
					<xsd:group ref="FareProductPriceGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="FareProductPriceGroup">
		<xsd:annotation>
			<xsd:documentation>Pricing Elements for a FARE PRODUCT PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="FareProductRef" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<!-- == CAPPING RULE PRICEs.============================= -->
	<xsd:complexType name="cappingRulePrices_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of CAPPING RULE PRICEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="strictContainmentAggregationStructure">
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="CappingRulePriceRef"/>
					<xsd:element ref="CellRef_"/>
					<xsd:element ref="CappingRulePrice"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="CappingRulePrice" abstract="false" substitutionGroup="FarePrice_">
		<xsd:annotation>
			<xsd:documentation>A set of all possible price features of a CAPPING RULE default total price, discount in value or percentage etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="CappingRulePrice_VersionedChildStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="VersionedChildGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="FarePriceGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="CappingRulePriceGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="CappingRulePriceIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="CappingRulePrice_VersionedChildStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a CAPPING RULE PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FarePrice_VersionedChildStructure">
				<xsd:sequence>
					<xsd:group ref="CappingRulePriceGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="CappingRulePriceGroup">
		<xsd:annotation>
			<xsd:documentation>Pricing Elements for a CAPPING RULE PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="CappingRuleRef" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
</xsd:schema>
