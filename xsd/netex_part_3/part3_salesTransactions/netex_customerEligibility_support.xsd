					<xsd:annotation>
						<xsd:documentation>Identifier of a USER PROFILE ELIGIBILITY.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
