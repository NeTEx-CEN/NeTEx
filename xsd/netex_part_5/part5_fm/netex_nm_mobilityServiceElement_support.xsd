	<!-- ====MOBILITY SERVICE ELEMENT.======================================================== -->
	<xsd:complexType name="mobilityServiceElementRefs_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of MOBILITY SERVICE ELEMENTs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="oneToManyRelationshipStructure">
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="MobilityServiceElementRef"/>
					<xsd:element ref="MobilityServiceElementInverseRef"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="MobilityServiceElementIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a MOBILITY SERVICE ELEMENT.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="PriceableObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="MobilityServiceElementRef" substitutionGroup="PriceableObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a MOBILITY SERVICE ELEMENT, used in a forward direction.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="MobilityServiceElementRefStructure"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="MobilityServiceElementRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a MOBILITY SERVICE ELEMENT  by direction</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="PriceableObjectRefStructure">
				<xsd:attribute name="ref" type="MobilityServiceElementIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a PRICEABLE ELEMENT.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="MobilityServiceElementInverseRef" substitutionGroup="PriceableObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a MOBILITY SERVICE ELEMENT, used in a backwards direction.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="MobilityServiceElementRefStructure"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- === DISTANCE MATRIX PRICE====================================================== -->
	<xsd:simpleType name="MobilityServiceElementPriceIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a MOBILITY SERVICE ELEMENT PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="FarePriceIdType"/>
	</xsd:simpleType>
	<xsd:element name="MobilityServiceElementPriceRef" type="MobilityServiceElementPriceRefStructure" substitutionGroup="FarePriceRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a MOBILITY SERVICE ELEMENT PRICE.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="MobilityServiceElementPriceRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a MOBILITY SERVICE ELEMENT PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="FarePriceRefStructure">
				<xsd:attribute name="ref" type="MobilityServiceElementPriceIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a MOBILITY SERVICE ELEMENT PRICE.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:schema>
