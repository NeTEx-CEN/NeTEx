	<!-- ===== MODEL PROFILE ======================================================== -->
	<!-- ======  MODEL EQUIPMENT PROFILE ====================================== -->
	<xsd:element name="ModelEquipmentProfile_" abstract="true" substitutionGroup="DataManagedObject">
		<xsd:annotation>
			<xsd:documentation>DUMMY TYPE tp Work around SG restrictions.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="DataManagedObjectStructure"/>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ModelEquipmentProfile" abstract="true">
		<xsd:annotation>
			<xsd:documentation>A set of characteristics of equipment installed on-board and characterising a MODEL EQUIPMENT PROFILE. +v1.2.2</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="ModelEquipmentProfile_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:group ref="ModelEquipmentProfileGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="ModelEquipmentProfileIdType">
						<xsd:annotation>
							<xsd:documentation>Identifier of MODEL EQUIPMENT PROFILE.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ModelEquipmentProfile_VersionStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Type for a MODEL EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:group ref="ModelEquipmentProfileGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="ModelEquipmentProfileGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a MODEL EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="MultilingualString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of VEHCILE MODEL.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfGears" type="xsd:nonNegativeInteger" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of Gears</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BabySeat" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there is a BabySeat/</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- =======CAR EQUIPMENT PROFILE================================================== -->
	<xsd:element name="CarEquipmentProfile" substitutionGroup="DataManagedObject">
		<xsd:annotation>
			<xsd:documentation>A set of characteristics of equipment installed on-board and characterising a CAR EQUIPMENT PROFILE. +v1.2.2</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="CarEquipmentProfile_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:element name="Name" type="MultilingualString" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Name of VEHCILE MODEL.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="NumberOfGears" type="xsd:nonNegativeInteger" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Number of Gears</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="BabySeat" type="xsd:boolean" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Whether there is a BabySeat/</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:group ref="CarEquipmentProfileGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="CarEquipmentProfileIdType">
						<xsd:annotation>
							<xsd:documentation>Identifier of CAR EQUIPMENT PROFILE.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="CarEquipmentProfile_VersionStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for a CAR EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ModelEquipmentProfile_VersionStructure">
				<xsd:group ref="CarEquipmentProfileGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="CarEquipmentProfileGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a CAR EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Transmission" type="TransmissionEnumeration" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of Transmission</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RoofRack" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>WHether vehicle has a roof rack.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WinterTyres" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether vehicel has winter tyres.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Chains" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether vehicel has remopvable chains.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AirConditioning" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether vehicel has remopvable chains.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SatNav" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether vehicel has satellite navigation.s.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======= CYCLE EQUIPMENT PROFILE================================================== -->
	<xsd:element name="CycleEquipmentProfile" substitutionGroup="DataManagedObject">
		<xsd:annotation>
			<xsd:documentation>A set of characteristics of equipment installed on-board and characterising a CYCLE EQUIPMENT PROFILE. +v1.2.2</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="CycleEquipmentProfile_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:element name="Name" type="MultilingualString" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Name of VEHCILE MODEL.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="NumberOfGears" type="xsd:nonNegativeInteger" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Number of Gears</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="BabySeat" type="xsd:boolean" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Whether there is a BabySeat/</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:group ref="CycleEquipmentProfileGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="CycleEquipmentProfileIdType">
						<xsd:annotation>
							<xsd:documentation>Identifier of CYCLE EQUIPMENT PROFILE.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="CycleEquipmentProfile_VersionStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for a CYCLE EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ModelEquipmentProfile_VersionStructure">
				<xsd:group ref="CycleEquipmentProfileGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="CycleEquipmentProfileGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a CYCLE EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="GearTypeDescriotion" type="MultilingualString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of gearing.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Battery" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there is a Battery</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Lamps" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there iare lights.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Helmet" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there iare is a helmet. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pump" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there iare is a pump. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Locker" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there iis a lockable luggae container.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Basket" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Whether there iis a basket.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
