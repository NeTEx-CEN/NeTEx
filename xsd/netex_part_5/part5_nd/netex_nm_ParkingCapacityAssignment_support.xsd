	<!-- ==== PARKING CAPACITYASSIGNMENT ============================================ -->
	<xsd:complexType name="parkingAreaCapacityAssignmentRefs_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of PARKING AREA CAPACITY ASSIGNMENTs. +v1.2.2</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="oneToManyRelationshipStructure">
				<xsd:sequence>
					<xsd:element ref="ParkingAreaCapacityAssignmentRef"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="ParkingAreaCapacityAssignmentIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a PARKING AREA CAPACITY ASSIGNMENT.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="AssignmentIdType"/>
	</xsd:simpleType>
	<xsd:element name="ParkingAreaCapacityAssignmentRef" type="ParkingAreaCapacityAssignmentRefStructure" substitutionGroup="AssignmentRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a PARKING AREA CAPACITY ASSIGNMENT. +v1.2.2</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="ParkingCapacityTypeEnumeration">
		<xsd:annotation>
			<xsd:documentation>Allowed values for PARKING CAPACITY TYPE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:normalizedString">
			<xsd:enumeration value="available"/>
			<xsd:enumeration value="inUse"/>
			<xsd:enumeration value="outOfService"/>
			<xsd:enumeration value="reserved"/>
			<xsd:enumeration value="unknown"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ParkingAreaCapacityAssignmentRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a reference to a PARKING AREA CAPACITY ASSIGNMENT. +v1.2.2</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="AssignmentRefStructure">
				<xsd:attribute name="ref" type="ParkingAreaCapacityAssignmentIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of PARKING CAPACITY ASSIGNMENT.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:schema>
