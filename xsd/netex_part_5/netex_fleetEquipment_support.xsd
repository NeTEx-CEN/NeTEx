	<!-- ===== MODEL EQUIPMENT PROFILE ======================================================== -->
	<xsd:simpleType name="ModelEquipmentProfileIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a MODEL EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="ModelEquipmentProfileRef" type="ModelEquipmentProfileRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a MODEL EQUIPMENT PROFILE +V1.2.2</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ModelEquipmentProfileRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a reference to a MODEL EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="ModelEquipmentProfileIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of referenced entity.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ===== CAR VEHICLE PROFILE ======================================================== -->
	<xsd:simpleType name="CarEquipmentProfileIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a CAR EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ModelEquipmentProfileIdType"/>
	</xsd:simpleType>
	<xsd:element name="CarEquipmentProfileRef" type="CarEquipmentProfileRefStructure" substitutionGroup="ModelEquipmentProfileRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a CAR EQUIPMENT PROFILE. +V1.2.2</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CarEquipmentProfileRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a reference to a CAR EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="ModelEquipmentProfileRefStructure">
				<xsd:attribute name="ref" type="CarEquipmentProfileIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of referenced entity.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ===== CAR VEHICLE PROFILE ======================================================== -->
	<xsd:simpleType name="CycleEquipmentProfileIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a CYCLE EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ModelEquipmentProfileIdType"/>
	</xsd:simpleType>
	<xsd:element name="CycleEquipmentProfileRef" type="CycleEquipmentProfileRefStructure" substitutionGroup="ModelEquipmentProfileRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a CYCLE EQUIPMENT PROFILE. +V1.2.2</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CycleEquipmentProfileRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a reference to a CYCLE EQUIPMENT PROFILE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="ModelEquipmentProfileRefStructure">
				<xsd:attribute name="ref" type="CycleEquipmentProfileIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of referenced entity.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:simpleType name="TransmissionEnumeration">
		<xsd:annotation>
			<xsd:documentation>Allowed values for Transmission</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:normalizedString">
			<xsd:enumeration value="automatic"/>
			<xsd:enumeration value="automatic4WheelDrive"/>
			<xsd:enumeration value="manual"/>
			<xsd:enumeration value="manual4WheelDrive"/>
		</xsd:restriction>
	</xsd:simpleType>
