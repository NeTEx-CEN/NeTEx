	<xsd:include schemaLocation="netex_deckPath_support.xsd"/>
	<xsd:include schemaLocation="netex_ifopt_navigationPath_version.xsd"/>
	<xsd:include schemaLocation="netex_ifopt_site_version.xsd"/>
