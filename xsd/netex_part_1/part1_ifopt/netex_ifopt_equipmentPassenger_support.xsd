		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="PassengerEquipmentRefStructure">
				<xsd:attribute name="ref" type="RubbishDisposalEquipmentIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a TICKETING EQUIPMENT.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
