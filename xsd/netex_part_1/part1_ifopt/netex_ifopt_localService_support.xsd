		<xsd:annotation>
			<xsd:documentation>Type for a reference to a LOCAL SERVICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="EquipmentRefStructure">
				<xsd:attribute name="ref" type="LocalServiceIdType" use="required">
					<xsd:annotation>
