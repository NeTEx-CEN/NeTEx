	<xsd:complexType name="InfrastructureLinkRestrictionRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a an INFRASTRUCTURE LINK RESTRICTION.</xsd:documentation>
		</xsd:annotation>
