	<!-- ======================================================================= -->
	<xsd:complexType name="dutyTypesInFrame_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for containment in frame of DUTY TYPEs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="containmentAggregationStructure">
				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="ContinuousDutyType"/>
					<xsd:element ref="SplitDutyType"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ==== DUTY TYPE ====================================================== -->
	<xsd:element name="DutyType_" type="DataManagedObjectStructure" abstract="true" substitutionGroup="DataManagedObject">
		<xsd:annotation>
			<xsd:documentation>Dummy Type. +v2.0</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DutyType" abstract="true" substitutionGroup="TypeOfEntity">
		<xsd:annotation>
			<xsd:documentation>A classification of a DUTY, in terms of working hours within the day. +v2.0</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="DutyType_ValueStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="TypeOfValueGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DutyTypeGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="DutyTypeIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="DutyType_ValueStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Type for a DUTY TYPE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="TypeOfEntity_VersionStructure">
				<xsd:sequence>
					<xsd:group ref="DutyTypeGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="DutyTypeGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a DUTY TYPE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StartTimebandRef" type="TimebandRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Start within TIMEBAND.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndTimebandRef" type="TimebandRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>End  within TIMEBAND</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== SPLIT DUTY TYPE ====================================================== -->
	<xsd:element name="SplitDutyType" abstract="false" substitutionGroup="DutyType_">
		<xsd:annotation>
			<xsd:documentation>A classification of a DUTY, in terms of working hours comprising several time bands separated in different DUTY PARTs by periods of unpaid time. +v2.0</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="SplitDutyType_ValueStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="TypeOfValueGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DutyTypeGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="SplitDutyTypeGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="SplitDutyTypeIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="SplitDutyType_ValueStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for a SPLIT DUTY TYPE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DutyType_ValueStructure">
				<xsd:sequence>
					<xsd:group ref="SplitDutyTypeGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="SplitDutyTypeGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a DUTY TYPE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StartUnpaidTimebandRef" type="TimebandRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Start within TIMEBAND.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndUnpaidTimebandRef" type="TimebandRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>End  within TIMEBAND</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ==== CONTINUOUS DUTY TYPE ====================================================== -->
	<xsd:element name="ContinuousDutyType" abstract="false" substitutionGroup="DutyType_">
		<xsd:annotation>
			<xsd:documentation>A classification of a DUTY, in terms of working hours within the day comprising a single period of paid work. +v2.0</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="ContinuousDutyType_ValueStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="TypeOfValueGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DutyTypeGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="ContinuousDutyTypeIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ContinuousDutyType_ValueStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for a CONTINUOUS DUTY TYPE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DutyType_ValueStructure"/>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
</xsd:schema>
