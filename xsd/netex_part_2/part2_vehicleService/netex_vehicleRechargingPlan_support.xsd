	<!-- ==== RECHARGING PLAN===================================================== -->
	<xsd:simpleType name="RechargingPlanIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a RECHARGING PLAN.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="RechargingPlanRef" type="RechargingPlanRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a RECHARGING PLAN. +v2.0</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="RechargingPlanRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a RECHARGING PLAN.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="RechargingPlanIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a RECHARGING PLAN.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ===== RECHARGING STEP ==================================================== -->
	<xsd:simpleType name="RechargingStepIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a RECHARGING STEP.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="RechargingStepRef" type="RechargingStepRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a RECHARGING STEP. +v2.0</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="RechargingStepRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Reference to a RECHARGING STEP.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="RechargingStepIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a RECHARGING STEP.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:simpleType name="RechargingProcessEnumeration">
		<xsd:annotation>
			<xsd:documentation>Allowed value for RECHARGING PROCESS. +v2.0</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:normalizedString">
			<xsd:enumeration value="changeBatteries"/>
			<xsd:enumeration value="powercable"/>
			<xsd:enumeration value="pantograph"/>
			<xsd:enumeration value="other"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
