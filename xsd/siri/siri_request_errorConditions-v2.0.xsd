	<xsd:include schemaLocation="../siri_utility/siri_types-v2.0.xsd"/>
	<xsd:include schemaLocation="siri_request_support-v2.0.xsd"/>
	<!-- ======================================================================= -->
	<xsd:annotation>
		<xsd:appinfo>
